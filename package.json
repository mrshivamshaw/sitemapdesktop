{
  "name": "sitemapdesktop",
  "version": "1.0.0",
  "main": "main.js",
  "description": "ABC Sitemap Generator",
  "author": "Shivam Shaw",
  "license": "MIT",
  "build": {
    "appId": "com.example.abcsitemap",
    "productName": "ABCSitemap",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/.*",
      "!**/*.map"
    ],
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "icon.icns",
      "identity": null
    },
    "linux": {
      "target": "AppImage",
      "icon": "icon.png",
      "category": "Utility"
    },
    "publish": null
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "debug": "electron --inspect=5858 .",
    "package": "electron-packager . ABCSitemap --platform=win32 --arch=x64 --icon=icon.ico --out=dist --overwrite"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "electron-store": "^8.0.0",
    "p-queue": "^8.1.0"
  },
  "devDependencies": {
    "electron": "^36.2.0",
    "electron-builder": "^26.0.12"
  }
}
